{
  "schemaVersion": 1,
  "id": "aoeemotes",
  "version": "${mod_version}",
  "name": "AOE Emotes",
  "description": "Discord style chat emotes for Minecraft chat. A fork of Fabric Emotes.",
  "authors": [
    "AlgidSpace",
    "TeamAOE"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/aof-emotes/",
    "sources": "https://github.com/quinn-semele/aoe-emotes/",
    "issues": "https://github.com/quinn-semele/aoe-emotes/issues/"
  },
  "license": "MIT",
  "icon": "aoe-emotes.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "dev.compasses.aofemotes.Main"
    ]
  },
  "mixins": [
    "aoe-emotes.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.15.11",
    "fabric-resource-loader-v0": ">=0.6.1",
    "minecraft": ">=1.21",
    "java": ">=21"
  },
  "breaks": {
    "fabricemotes": "*"
  }
}
